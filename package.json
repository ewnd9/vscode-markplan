{
  "name": "markplan",
  "displayName": "markplan",
  "description": "utils for markdown",
  "version": "0.0.1",
  "publisher": "ewnd9",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:extension.helloWorld",
    "onCommand:extension.openWebview",
    "onCommand:extension.openTree"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.openWebview",
        "title": "Markplan: Open Webview"
      },
      {
        "command": "extension.openTree",
        "title": "Markplan: Open Tree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn build",
    "build": "tsc -p ./",
    "build:watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p .",
    "test": "yarn build && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vsce": "^1.62.0",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "line-column": "^1.0.2"
  }
}
